{
  "name": "Hasher",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Developer's toolbox",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardWrite"
  ],
  "icons": {
     "16": "images/icon16.png",
     "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
